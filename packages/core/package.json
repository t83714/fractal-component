{
  "name": "fractal-component",
  "version": "4.4.2",
  "description": "`fractal-component` is a javascript library that can help you to encapsulate decoupled resuable UI component easily",
  "main": "dist/fractal-component.cjs.js",
  "module": "dist/fractal-component.esm.js",
  "unpkg": "dist/fractal-component.min.umd.js",
  "react-native": "dist/fractal-component.react-native.js",
  "files": [
    "dist",
    "src/**/*.d.ts"
  ],
  "scripts": {
    "prettier": "prettier --write 'src/*.js' 'src/*.ts'",
    "eslint": "eslint src/ __tests__/",
    "dtslint": "dtslint src --onlyTestTsNext",
    "test": "cd ../.. && jest",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "rollup -c -m",
    "prepush": "npm run test",
    "prepack": "npm run test && bundlesize && cp ../../README.md ./README.md && cp ../../LICENSE ./LICENSE",
    "postpack": "unlink ./README.md && unlink ./LICENSE",
    "prepare": "npm run build",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:major": "npm version major && npm publish && git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/t83714/fractal-component.git"
  },
  "keywords": [
    "javascript",
    "redux",
    "react",
    "saga",
    "fractal component",
    "encapsulation",
    "namespaced action",
    "namespaced store",
    "multicast action",
    "multicast messaging",
    "reducer hot loading",
    "saga hot loading"
  ],
  "homepage": "https://t83714.github.io/fractal-component/",
  "author": "Jacky Jiang <t83714@gmail.com>",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/t83714/fractal-component/issues"
  },
  "bundlesize": [
    {
      "path": "./dist/fractal-component.min.umd.js",
      "maxSize": "16.5 Kb"
    }
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "lodash": "^4.17.10",
    "mitt": "^1.1.3",
    "object-path": "^0.11.4",
    "object-path-immutable": "^3.0.0",
    "prop-types": "~15.6.0",
    "react": "~16.8.0",
    "redux": "~4.0.0",
    "redux-saga": "~1.0.0"
  },
  "peerDependencies": {
    "prop-types": "~15.6.0",
    "react": "~16.8.0",
    "redux-saga": "~1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@types/react": "^16.8.0",
    "babel-eslint": "^10.0.0",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "bundlesize": "^0.17.2",
    "dtslint": "^0.4.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.13.0",
    "eslint-plugin-jest": "^22.2.0",
    "eslint-plugin-react": "^7.12.0",
    "jest": "^24.0.0",
    "jest-environment-enzyme": "^7.0.2",
    "jest-enzyme": "^7.0.2",
    "jsdom": "^15.0.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.16.0",
    "redux-devtools-extension": "^2.13.5",
    "rimraf": "^2.6.2",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^4.0.4",
    "typescript": "^3.3.1"
  },
  "typings": "./src/index.d.ts",
  "npmName": "fractal-component",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ]
}
